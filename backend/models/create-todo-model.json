{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+",
      "minLength": 1
    },
    "dueDate": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"

    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}
